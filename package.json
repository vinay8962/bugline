{
  "name": "bugline-monorepo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:client",
    "dev:client": "vite",
    "dev:server": "cd server && npm run dev",
    "dev:all": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build": "vite build",
    "build:client": "vite build",
    "build:server": "cd server && npm run build",
    "build:all": "npm run build:client && npm run build:server",
    "start": "npm run start:client",
    "start:client": "vite preview",
    "start:server": "cd server && npm start",
    "start:all": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "lint": "npm run lint:client && npm run lint:server",
    "lint:client": "eslint .",
    "lint:server": "cd server && npm run lint",
    "lint:fix": "npm run lint:fix:client && npm run lint:fix:server",
    "lint:fix:client": "eslint . --fix",
    "lint:fix:server": "cd server && npm run lint:fix",
    "test": "npm run test:client && npm run test:server",
    "test:client": "vitest",
    "test:server": "cd server && npm test",
    "clean": "npm run clean:client && npm run clean:server",
    "clean:client": "rm -rf dist node_modules/.vite",
    "clean:server": "cd server && rm -rf node_modules/.cache",
    "install:all": "npm install && cd server && npm install",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.10.0",
    "framer-motion": "^12.23.9",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vitest": "^1.6.1"
  }
}
